{
  "name": "chrome-markdown-emojis",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^6.2.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/markdown-it": "^12.2.1",
    "@types/markdown-it-emoji": "^2.0.2",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^5.0.0",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "INLINE_RUNTIME_CHUNK=false craco build && npm run scss-content",
    "test": "jest -i",
    "test:coverage": "jest -i --coverage --coverage-reporters=cobertura",
    "eject": "react-scripts eject",
    "scss-content": "node-sass ./src/content/ -o ./build/static/css/",
    "scss-content:watch": "node-sass --watch ./src/content/ -o ./build/static/css/"
  },
  "eslintConfig": {},
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/chrome": "^0.0.154",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.2",
    "jest": "^27.5.1",
    "jest-environment-jsdom": "^27.5.1",
    "node-sass": "^5.0.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.4.2"
  },
  "jest": {
    "clearMocks": true,
    "rootDir": "./src",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  }
}
