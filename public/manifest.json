{
  "action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "background": {
    "service_worker": "./static/js/background.js"
  },
  "content_scripts": [
    {
      "css": [
        "./static/css/content.css"
      ],
      "js": [
        "./static/js/content.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "description": "Allow to use markdown emojis code like :tada: ðŸŽ‰ in text inputs",
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "manifest_version": 3,
  "name": "Chrome markdown emojis",
  "options_page": "index.html#options",
  "permissions": [
    "storage"
  ],
  "short_name": "Chrome Emojis",
  "version": "0.11"
}
