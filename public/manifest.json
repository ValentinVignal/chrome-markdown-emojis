{
  "short_name": "React App",
  "name": "Create React App Sample",
  "manifest_version": 3,
  "version": "0.1",
  "action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "./static/js/content.js"
      ]
    }
  ],
  "background": {
    "service_worker": "background-wrapper.js"
  }
}